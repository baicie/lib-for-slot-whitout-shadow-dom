{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve", // 保留 JSX，不做特殊处理
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "esnext"]
  }
}
